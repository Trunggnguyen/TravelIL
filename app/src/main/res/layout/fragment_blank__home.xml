<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#E8E8E8"
    android:orientation="vertical"
    android:layout_height="wrap_content"
    tools:context=".Fragment.Fragment_Home.BlankFragment_Home">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <fragment
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:name="com.example.travelil.Fragment.Fragment_Home.BlankFragment_Banner"
            android:id="@+id/fragmentbanner"
            />

        <fragment
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:name="com.example.travelil.Fragment.Fragment_Home.BlankFragment_Choice_iteam"
            android:id="@+id/fragmentchoiceiteam"
            />
        <fragment
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:name="com.example.travelil.Fragment.Fragment_Home.BlankFragment_Place"
            android:id="@+id/fragmentplace"
            />
        <fragment
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:name="com.example.travelil.Fragment.BlankFragment_Story"
            android:id="@+id/fragmentplacesss"
            />
        <fragment
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:name="com.example.travelil.Fragment.BlankFragment_BannerTrGr"
            android:id="@+id/fragmentgr"
            />
        <fragment
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:name="com.example.travelil.Fragment.BlankFragment_PlaceHot"
            android:id="@+id/fragmenplacehot"
            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_50sdp"

            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_70sdp"

            />

    </LinearLayout>

</ScrollView>